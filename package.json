{
  "name": "yt-event-aggregator",
  "version": "0.1.0",
  "description": "Chrome extension to aggregate events from YouTube subscriptions",
  "type": "module",
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:report": "npx playwright test --reporter=html && npx playwright show-report",
    "lint": "eslint src/ tests/",
    "knip": "knip",
    "knip:production": "knip --production"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.10.0",
    "eslint": "^9.39.1",
    "knip": "^5.70.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.48.0"
  },
  "keywords": [
    "youtube",
    "chrome-extension",
    "events",
    "subscriptions"
  ],
  "author": "",
  "license": "MIT"
}
